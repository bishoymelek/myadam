{
  "name": "adam-painter-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Adam Painter Booking Frontend with React, TypeScript, and Ant Design",
  "type": "module",
  "author": "Adam Painter Booking System",
  "keywords": ["react", "typescript", "antd", "booking", "painter", "frontend"],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "start": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc -b --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "echo 'No tests configured'"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "antd": "^5.27.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
