{
  "version": 2,
  "builds": [
    {
      "src": "./api/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "cd api && npm install",
  "buildCommand": "cd api && npm run build"
}